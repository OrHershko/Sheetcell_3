<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.actionline.ActionLineController">
   <children>
      <Label fx:id="selectedCellId" minWidth="-Infinity" text="Selected cell ID: " />
      <Label fx:id="originalCellValue" minWidth="-Infinity" text="Original cell value: " />
      <TextField fx:id="textField" minWidth="100.0" prefWidth="300.0" />
      <Button fx:id="updateCellButton" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#updateCell" text="Update Cell" />
      <Label fx:id="lastUpdateCellVersion" minWidth="-Infinity" text="Last update cell version: " />
      <Button fx:id="dynamicCalculationButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#dynamicCalculationOnClick" text="Dynamic Calculation" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
