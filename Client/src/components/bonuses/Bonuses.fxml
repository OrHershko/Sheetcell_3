<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<HBox fx:id="bonusesComponent" spacing="10.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.bonuses.BonusesController">
   <children>
      <MenuButton fx:id="skinsMenuButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Skins">
        <items>
           <MenuItem fx:id="defaultSkinMenuItem" text="Basic" />
           <MenuItem fx:id="darkSkinMenuItem" text="Night Owl" />
           <MenuItem fx:id="lightSkinMenuItem" text="Sunrise" />
        </items>
      </MenuButton>
      <MenuButton fx:id="animationsMenuButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Animations">
        <items>
          <MenuItem fx:id="animationsOnMenuItem" mnemonicParsing="false" text="On" />
          <MenuItem fx:id="animationsOffMenuItem" mnemonicParsing="false" text="Off" />
        </items>
      </MenuButton>
   </children>
   <padding>
      <Insets bottom="30.0" left="10.0" right="30.0" top="30.0" />
   </padding>
</HBox>
